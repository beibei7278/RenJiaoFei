    <RelativeLayout
        android:id="@+id/rel_setting_clean_cache"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        android:background="@color/white"
        android:orientation="horizontal"
        android:padding="@dimen/main_size_ten" >

        <TextView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="@dimen/main_size_fifteen"
            android:text="清除缓存"
            android:textColor="@color/black"
            android:textSize="@dimen/main_text_fifteen" />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_gravity="center_vertical"
            android:background="@drawable/jiantou" />
    </RelativeLayout>

    <Button
        android:id="@+id/bt_tuichu"
        android:layout_width="fill_parent"
        android:layout_height="40dp"
        android:layout_gravity="end"
        android:layout_marginLeft="@dimen/main_size_thirty"
        android:layout_marginRight="@dimen/main_size_thirty"
        android:layout_marginTop="@dimen/main_size_twenty_three"
        android:background="@drawable/denglu"
        android:clickable="true"
        android:text="@string/bt_hint_tuichu"
        android:textColor="@color/white"
        android:textSize="@dimen/main_text_fifteen" />

    <Button
        android:id="@+id/pay_zhifu"
        android:layout_width="fill_parent"
        android:layout_height="40dp"
        android:visibility="gone"
        android:text="aaa" />

    <WebView
        android:id="@+id/login_exit"
        android:layout_width="fill_parent"
        android:layout_height="0dp"
        android:background="@color/color_my_garden_background" />

</LinearLayout>